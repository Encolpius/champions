    "defaultActions": [
      {
        "actionListId": "discardCard",
        "label": "discard",
        "condition": ["$ACTIVE_CARD.inPlay"]
      }
    ],
    "actionLists": {
      "drawCard": [
        ["DRAW_CARD"]
      ],
      "nextStep": [
        ["GAME_INCREASE_VAL", "stepIndex", 1],
        ["COND",
          ["EQUAL", "$GAME.stepIndex", ["LENGTH", "$GAME.gameDef.steps"],
          ["GAME_SET_VAL", "stepIndex", 0]
        ]
        ],
        ["DEFINE", "$STEP_INDEX", "$GAME.stepIndex"],
        ["DEFINE", "$STEP", "$GAME.gameDef.steps.[$STEP_INDEX]"],
        ["GAME_ADD_MESSAGE", "$PLAYER_N", " set the round step to ", "$STEP.text", "."]
      ],
      "prevStep": [
        ["GAME_DECREASE_VAL", "stepIndex", 1],
        ["COND",
          ["LESS_THAN", "$GAME.stepIndex", 0],
          ["GAME_SET_VAL", "stepIndex", ["MINUS", ["LENGTH", "$GAME.gameDef.steps"], 1]]
        ],
        ["DEFINE", "$STEP_INDEX", "$GAME.stepIndex"],
        ["DEFINE", "$STEP", "$GAME.gameDef.steps.[$STEP_INDEX]"],
        ["GAME_ADD_MESSAGE", "$PLAYER_N", " set the round step to ", "$STEP.text", "."]
      ],
      "clearTargets": [
        ["FOR_EACH_KEY_VAL", "$CARD_ID", "$CARD", "$CARD_BY_ID",
          ["GAME_SET_VAL", "cardById", "$CARD_ID", "targeting", "$PLAYER_N", false]
        ],
        ["GAME_ADD_MESSAGE", "$PLAYER_N", " cleared their targets."]
      ],
      "zeroTokens": [
        ["GAME_SET_VAL", "$ACTIVE_TOKENS_PATH", {}],
        ["GAME_ADD_MESSAGE", "$PLAYER_N", " removed all tokens from ", "$ACTIVE_FACE.name", "."]
      ],
      "toggleRotate": [
        ["COND",
          ["AND", ["EQUAL", "$ACTIVE_CARD.rotation", 90], "$ACTIVE_CARD.inPlay"],
          [
            ["GAME_SET_VAL", "$ACTIVE_CARD_PATH", "rotation", 0],
            ["GAME_ADD_MESSAGE", "$PLAYER_N", " rotated ", "$ACTIVE_FACE.name", "."]
          ],
          ["AND", ["EQUAL", "$ACTIVE_CARD.rotation", 0], "$ACTIVE_CARD.inPlay"],
          [
            ["GAME_SET_VAL", "$ACTIVE_CARD_PATH", "rotation", 90],
            ["GAME_ADD_MESSAGE", "$PLAYER_N", " rotated ", "$ACTIVE_FACE.name", "."]
          ]
        ]
      ],
      "flipCard": [
        ["COND",
          ["EQUAL", "$ACTIVE_CARD.currentSide", "A"],
          [
            ["GAME_ADD_MESSAGE", "$PLAYER_N", " flipped ", "$ACTIVE_FACE.name", " facedown."],
            ["GAME_SET_VAL", "$ACTIVE_CARD_PATH", "currentSide", "B"]
          ],
          true,
          [
            ["GAME_SET_VAL", "ACTIVE_CARD_PATH", "currentSide", "A"],
            ["GAME_ADD_MESSAGE", "$PLAYER_N", " flipped ", "$ACTIVE_FACE.name", " faceup."]
          ]
        ]
      ],
      "targetCard": [
        ["GAME_SET_VAL", "$ACTIVE_CARD_PATH", "targeting", "$PLAYER_N", true],
        ["GAME_ADD_MESSAGE", "$PLAYER_N", " targeted ", "$ACTIVE_FACE.name", "."]
      ],
      "addToScoringArea": [
        ["COND",
            ["EQUAL", "$PLAYER_DATA.team", "red"],
            ["MOVE_CARD", "$ACTIVE_CARD_ID", "sharedScoringRed", 0, 0],
            ["EQUAL", "$PLAYER_DATA.team", "blue"],
            ["MOVE_CARD", "$ACTIVE_CARD_ID", "sharedScoringBlue", 0, 0]
        ]
      ],
      "discardCard": [
        ["COND",
          ["EQUAL", "$ACTIVE_CARD.cardIndex", 0],
          [
            ["DEFINE", "$STACK_ID", "$ACTIVE_CARD.stackId"],
            ["DEFINE", "$CARD_IDS", "$GAME.stackById.$STACK_ID.cardIds"],
            ["FOR_EACH_VAL", "$CARD_ID", "$CARD_IDS",
              [
                ["DEFINE", "$CARD", "$GAME.cardById.$CARD_ID"],
                ["COND",
                  ["EQUAL", "$CARD.discardGroupId", null],
                  ["GAME_ADD_MESSAGE", "$PLAYER_N", " failed to discard ", ["FACEUP_NAME_FROM_CARD_ID", "$CARD_ID"], " because it is not associated with a discard pile. Please drag the card instead."],
                  true,
                  [
                    ["DEFINE", "$CURRENT_SIDE", "$CARD.currentSide"],
                    ["GAME_ADD_MESSAGE", "$PLAYER_N", " discarded ", "$CARD.sides.$CURRENT_SIDE.name", "."],
                    ["DISCARD_CARD", "$CARD_ID"]
                  ]
                ]
              ]
            ]
          ],
          true,
          ["COND",
            ["EQUAL", "$ACTIVE_CARD.discardGroupId", null],
            ["GAME_ADD_MESSAGE", "$PLAYER_N", " failed to discard ", "$ACTIVE_FACE.name", " because it is not associated with a discard pile. Please drag the card instead."],
            true,
            [
              ["GAME_ADD_MESSAGE", "$PLAYER_N", " discarded ", "$ACTIVE_FACE.name", "."],
              ["DISCARD_CARD", "$ACTIVE_CARD_ID"]
            ]
          ]
        ] 
      ],
      "detach": [
        ["COND",
          ["GREATER_THAN", "$ACTIVE_CARD.cardIndex", 0],
          [
            ["MOVE_CARD", "$ACTIVE_CARD_ID", "$ACTIVE_CARD.groupId", ["ADD", "$ACTIVE_CARD.stackIndex", 1], 0],
            ["GAME_ADD_MESSAGE", "$PLAYER_N", " detached ", "$ACTIVE_FACE.name", "."]
          ]
        ]
      ],
      "togglePeeking": [
        ["DEFINE", "$GROUP_ID", "$ACTIVE_CARD.groupId"],
        ["DEFINE", "$GROUP_NAME", ["$GAME.groupById.$GROUP_ID.name"]],
        ["COND",
          ["EQUAL", "$ACTIVE_CARD.peeking.$PLAYER_N", true],
          [
            ["GAME_ADD_MESSAGE", "$PLAYER_N", " stopped peeking at a card in ", "$GROUP_NAME"],
            ["GAME_SET_VAL", "$ACTIVE_CARD_PATH", "peeking", "$PLAYER_N", false]
          ],
          true,
          [
            ["GAME_ADD_MESSAGE", "$PLAYER_N", " started peeking at a card in ", "$GROUP_NAME"],
            ["GAME_SET_VAL", "$ACTIVE_CARD_PATH", "peeking", "$PLAYER_N", true]
          ]
        ]
      ],
      "deleteCard": [
        ["GAME_ADD_MESSAGE", "$PLAYER_N", " deleted ", "$ACTIVE_FACE.name", "."],
        ["DELETE", "$ACTIVE_CARD_ID"]
      ]
    },
